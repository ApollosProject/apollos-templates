    "nodemon": "1.18.7",
    "prettier": "1.14.2",
    "prop-types": "15.6.1",
    "react-dom": "16.6.3",
    "react-native-storybook-loader": "1.8.0",
    "react-test-renderer": "16.13.1",
    "waait": "1.0.2",
    "yalc": "^1.0.0-pre.47"
  },
  "rnpm": {
    "assets": [
      "assets/fonts"
    ]
  }
}
