    "node-fetch": "2.6.1",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-config": "^0.11.7",
    "react-native-device-info": "^4.0.1",
    "react-native-geolocation-service": "^5.1.1",
    "react-native-gesture-handler": "^1.10.3",
