{
  "name": "apollos-church-api",
  "version": "1.0.0",
  "private": true,
  "main": "lib/index.js",
  "scripts": {
    "setup": "./scripts/init.sh",
    "start": "yarn start:prod",
    "start:dev": "linkemon ./src/index.js --exec babel-node --delay 2 -e js,yaml,json,yml",
    "start:prod": "NODE_ENV=production node ./lib/index.js",
    "postinstall": "yarn build",
    "link-packages": "node ./scripts/link-packages",
    "unlink-packages": "node ./scripts/link-packages remove",
    "build": "NODE_ENV=production rm -rf ./lib && mkdir -p lib && babel ./src -d ./lib",
    "test": "echo \"No tests in apollos-church-api, all tested code is located in @apollosproject npm modules\"",
    "lint": "eslint ./src --ext .js"
  },
  "engines": {
    "node": ">=8.x.x"
  },
  "dependencies": {
    "@apollosproject/bugsnag": "^2.0.0-alpha.6",
    "@apollosproject/config": "^2.0.0-alpha.6",
    "@apollosproject/data-connector-algolia-search": "^2.0.0-alpha.6",
    "@apollosproject/data-connector-analytics": "^2.0.0-alpha.6",
    "@apollosproject/data-connector-bible": "^2.0.0-alpha.6",
    "@apollosproject/data-connector-church-online": "^2.0.0-alpha.6",
    "@apollosproject/data-connector-cloudinary": "^2.0.0-alpha.6",
    "@apollosproject/data-connector-onesignal": "^2.0.0-alpha.6",
    "@apollosproject/data-connector-passes": "^2.0.0-alpha.6",
    "@apollosproject/data-connector-redis-cache": "^2.0.0-alpha.6",
    "@apollosproject/data-connector-rock": "^2.0.0-alpha.6",
    "@apollosproject/data-connector-twilio": "^2.0.0-alpha.6",
    "@apollosproject/data-schema": "^2.0.0-alpha.6",
    "@apollosproject/rock-apollo-data-source": "^2.0.0-alpha.6",
    "@apollosproject/server-core": "^2.0.0-alpha.6",
    "apollo-server": "^2.14.2",
    "apollo-server-express": "2.14.2",
    "color": "3.0.0",
    "dotenv": "6.0.0",
    "express": "^4.17.0",
    "graphql": "14.6.0",
    "prettier": "1.12.1",
    "randomcolor": "0.5.3"
  },
  "devDependencies": {
    "@apollosproject/apollo-server-env-mock": "^2.0.0-alpha.5",
    "@apollosproject/babel-preset-apollos": "^2.0.0-alpha.5",
    "@apollosproject/eslint-config": "^2.0.0-alpha.5",
    "@babel/cli": "^7.4.5",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.7.4",
    "babel-eslint": "^10.0.3",
    "babel-plugin-module-resolver": "^3.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-babel-module": "^5.0.0-beta.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.15.2",
    "eslint-plugin-prettier": "^2.6.0",
    "linkemon": "^0.1.1",
    "nodemon": "1.18.7"
  }
}
